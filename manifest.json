{
    "manifest_version": 3,
    "name": "Youtube Reklam Atlayıcı",
    "version": "1.3",
    "icons": {
      "48": "assets/icon.png",
      "128": "assets/icon.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "assets/icon.png",
        "128": "assets/icon.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  